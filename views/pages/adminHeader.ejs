    <div class="main-body">
      <h2>Dashboard</h2>
      <div class="promo_card">
        <h1>Welcome Seif Nasser </h1>
        <span>This is the Admin Dashboard</span>
        <!-- <button>Learn More</button> -->
      </div>

      <div class="history_lists">
        <div class="list1">
          <div class="row">
            <h4>Users</h4>
            <a href="adminUsers.html">See all</a>
          </div>
          <table>
            <thead>
              <tr>
                <th>#</th>
                <th>UserName</th>
                <th>Password</th>
                <th>Email</th>
              </tr>
            </thead>
            <tbody>
              <% if (Users.length> 0){%>
                <% var count=1; %>
                  <% Users.forEach(Client=>{%>
                      <% if (Client.type=='client' ){%>
                        <tr>
                          <td>
                            <%=count%>
                          </td>
                          <td>
                            <%=Client.username%>
                          </td>
                          <td>
                            <%=Client.password%>
                          </td>
                          <td>
                            <%=Client.email%>
                          </td>
                        </tr>
                        <% count++;%>
                        <% } else { %>
                          <p>No Users found </p>
                          <% }%>
                              <% })%>
              <% } else { %>
                <p>No Users found </p>
               <% }%>
            </tbody>
          </table>
        </div>

        <div class="list2">
          <div class="row">
            <h4>Admin</h4>
            <!-- <a href="#">See all</a> -->
          </div>
          <table>
